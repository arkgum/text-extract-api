	"item": [
		{
			"name": "01 - OCR Upload (Multipart)",
			"event": [
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"const jsonData = pm.response.json();",
							"if (jsonData.task_id) {",
							"    pm.collectionVariables.set(\"last_task_id\", jsonData.task_id);",
							"    pm.test(\"task_id captured\", function () {",
							"        pm.expect(jsonData.task_id).to.be.a('string');",
							"    });",
							"} else {",
							"    pm.test(\"Response contains task_id\", function () {",
							"        pm.expect.fail('task_id missing in response');",
							"    });",
							"}"
						]
					}
				}
			],
